<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.d3ifcool.halojantung">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Halo_Jantung">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".JadwalActivity"
            android:label="Jadwal Makan" />
        <activity
            android:name=".RegisterActivity"
            android:label="Register" />
        <activity
            android:name=".LoginActivity"
            android:label="Login" />
        <activity android:name=".MenuActivity" />
        <activity
            android:name=".FingerprintActivity"
            android:label="Scan Sidik Jari" />
        <activity android:name=".AlarmActivity"></activity>

        <receiver android:name=".AlarmPemberitahuan"/>
        <receiver android:name=".AlarmToastReceiver"/>

    </application>

</manifest>